    "@storybook/addon-viewport": "7.0.12",
    "@storybook/blocks": "7.0.12",
    "@storybook/react": "7.0.12",
    "@storybook/react-vite": "7.0.12",
    "@storybook/testing-library": "0.1.0",
    "@types/react": "17.0.48",
