    "@storybook/components": "7.0.12",
    "@storybook/core-events": "7.0.12",
    "@storybook/manager-api": "7.0.12",
    "@storybook/preview-api": "7.0.12",
    "@storybook/theming": "7.0.12",
    "mobx": "6.9.0",
    "mobx-react-lite": "3.4.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.12",
