{
  "name": "vite-latest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 4000 --no-open",
    "start": "yarn run dev",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev --port 6006 --no-open --disable-telemetry --host 0.0.0.0",
    "storybook:build": "storybook build --disable-telemetry",
    "storybook:build:watch": "yarn run storybook:build --watch",
    "storybook:preview": "vite preview --port 9009 --outDir storybook-static --host 0.0.0.0",
    "storybook:build:preview": "yarn run storybook:build && yarn run storybook:preview"
  },
  "dependencies": {
    "@emotion/babel-plugin": "11.11.0",
